{
  "name": "chords-genius",
  "version": "0.0.1",
  "description": "This service allows you to find chords you can play over a song.",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.js",
    "test": "jest -i --silent",
    "test-watch": "jest -i --silent --watchAll",
    "test-watch-verbose": "jest -i --watchAll",
    "clear-cache-and-test": "jest --clearCache && jest -i"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gkermo/chords-genius.git"
  },
  "author": "gkermo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gkermo/chords-genius/issues"
  },
  "homepage": "https://github.com/gkermo/chords-genius#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi": "^7.1.0",
    "helmet": "^4.2.0",
    "http-status-codes": "^2.1.4",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jsdom": "^19.0.0",
    "nocache": "^2.1.0",
    "supertest": "^6.0.1",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1"
  }
}
